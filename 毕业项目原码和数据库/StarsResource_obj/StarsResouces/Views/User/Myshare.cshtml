                                评论（@item.Comment.Count()）
                                <svg t="1592216896882" class="icon" viewBox="0 0 1024 1024" version="1.1" xmlns="http://www.w3.org/2000/svg" p-id="7848" width="16" height="16"><path d="M529.066667 213.333333a341.333333 341.333333 0 1 1-341.333334 341.333334 341.333333 341.333333 0 0 1 341.333334-341.333334m0-85.333333a426.666667 426.666667 0 1 0 426.666666 426.666667 426.666667 426.666667 0 0 0-426.666666-426.666667z" fill="#515151" p-id="7849"></path><path d="M529.066667 298.666667a42.666667 42.666667 0 0 0-42.666667 42.666666v213.333334a42.666667 42.666667 0 0 0 85.333333 0V341.333333a42.666667 42.666667 0 0 0-42.666666-42.666666z" fill="#515151" p-id="7850"></path><path d="M742.4 512h-213.333333a42.666667 42.666667 0 1 0 0 85.333333h213.333333a42.666667 42.666667 0 0 0 0-85.333333z" fill="#515151" p-id="7851"></path></svg>
                                @string.Format("{0:D}", item.Releasetime)
                                <svg t="1593238367881" class="icon" viewBox="0 0 1024 1024" version="1.1" xmlns="http://www.w3.org/2000/svg" p-id="2161" width="16" height="16"><path d="M954.88 237.568c0-60.416-56.832-109.568-126.464-109.568H195.584c-70.144 0-126.464 49.152-126.464 109.568v548.352c0 60.416 56.832 109.568 126.464 109.568h632.832c70.144 0 126.464-49.152 126.464-109.568V237.568z m-803.84-28.16c11.776-10.24 28.16-16.384 45.056-15.872h632.832c34.816 0 63.488 14.336 63.488 44.544v246.784h-50.176l-54.272-47.104c-12.288-10.752-32.256-10.752-44.544 0-3.584 3.072-6.144 6.656-7.68 10.752l-38.912 101.888L604.16 390.144c-7.68-13.312-26.624-18.944-42.496-12.288-7.168 3.072-12.288 8.192-15.36 14.336l-92.16 199.68-101.376-307.2c-4.608-14.336-22.528-23.04-38.912-18.944-8.704 2.048-15.872 7.168-19.456 14.336l-117.76 204.288H133.12V247.808c-1.024-14.336 6.144-28.16 17.92-38.4z m725.504 611.84c-7.68 7.68-31.232 9.728-48.128 9.728H195.584c-16.896 0-32.768-5.632-45.056-15.872s-18.432-24.064-18.432-38.912v-236.544h63.488c11.776 0 23.04-5.632 28.16-15.36l90.112-156.16 104.448 316.416c3.584 11.264 15.36 19.456 28.672 19.968h1.536c12.8 0 24.576-6.656 29.184-17.408L578.56 468.992l95.232 164.864c7.68 13.312 26.624 18.944 42.496 12.288 7.68-3.072 13.312-8.704 15.872-15.872l47.104-122.368 27.136 23.552c6.144 5.12 13.824 8.192 22.528 8.192h62.976v236.544c0 14.336-9.728 40.448-14.336 44.544l-1.024 0.512z" p-id="2162"></path></svg>
                                状态:@if (item.Rstate == 0)
                                {
                                    <label style="color:springgreen">通过</label>
                                }
                                else if (item.Rstate == 1)
                                {
                                    <label style="color:yellow">审核中</label>
                                }
                                else
                                {
                                    <label style="color:red">未通过</label>
                                }
                            </span>
                        </dd>
                    </dl>
                </a>
                <a href="/User/MyshareEdit?id=@item.ResoucesID" class="btn btn-outline-info">编辑</a>
                <a href="/User/MyshareDelete?id=@item.ResoucesID" class="btn btn-outline-danger" onclick="return confirm('确定删除？')">删除</a>
            </div>
        }

        <div style="width:100%;height:100px;padding:10px;margin-bottom:5px;text-align:center;background-color:white">
            当前页/总页数（ @(Model.PageCount < Model.PageNumber ? 0 : Model.PageNumber) / @Model.PageCount ）
            @Html.PagedListPager(Model, page => Url.Action("Myshare", new
       {
           page
       }))
        </div>
    </div>
}


